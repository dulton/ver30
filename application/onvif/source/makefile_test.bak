#---------------------------------------------------------------------
SRC := ../source

#---------------------------------------------------------------------
CC := g++
AR := ar

#---------------------------------------------------------------------
INCS :=\
	-I$(SRC)/common\
	-I$(SRC)/../include\
	-I$(SRC)/generation \
	-I$(SRC)/../../../../ipc_fw_v3.x/\
