	../../../../../module/gmi/log_utils/library/liblog_x86_d.a\
	../../../../../module/gmi/command_pipeline/library/libcommand_pipeline_x86_d.a\
  ../../../../../osal/library/libosal_x86_d.a

all: $(TARGETS)

$(TARGETS):$(OBJS)
	$(CC) -o $@ $(DEFS) $(CFLAGS) $(LDFLAGS) $(OBJS) $(SYS_LIBS)
  
.PHONY:clean

clean:
	rm -f $(TARGETS) ./*/*.o ./*/*.c~ ./*/*.h~ *.o	
	
